{
  "name": "valuva_backend",
  "version": "1.0.0",
  "description": "backend of valuva ",
  "license": "MIT",
  "author": "mithun",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"cd ../frontend && npm run dev\"",
    "build": "tsc",
    "build:full": "npm run build && cd ../frontend && npm run build",
    "start": "node dist/server.js",
    "start:full": "concurrently \"npm run start\" \"cd ../frontend && npm run start\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "backup:db": "ts-node src/scripts/backup-database.ts",
    "install:all": "npm install && cd ../frontend && npm install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@prisma/client": "^5.22.0",
    "@react-email/components": "^1.0.2",
    "@sentry/node": "^8.55.0",
    "@sentry/profiling-node": "^8.55.0",
    "@types/compression": "^1.8.1",
    "@types/express-session": "^1.18.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.12",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "razorpay": "^2.9.6",
    "react": "^19.2.3",
    "react-email": "^5.1.0",
    "redis": "^5.10.0",
    "sharp": "^0.33.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1",
    "web-push": "^3.6.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.7",
    "@types/web-push": "^3.6.4",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
